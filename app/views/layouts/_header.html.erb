<header class="navbar navbar-fixed-top navbar-inverse" >
      <div class="container">
        <%= link_to "Focus Fitness", root_path, id: "logo" %>
        <nav>
          <ul class="nav navbar-nav navbar-right">
            <li><%= link_to "Home", root_path %></li>
           
            <% if logged_in?%>
             <li><%= link_to "Articles", articles_path %></li>
              <li><%= link_to "Assessments", assessments_path %></li>
            <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  Classes <b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <li><%= link_to "One to one", allappointments_path %></li>
                  <li><%= link_to "Group",allclasses_path  %></li>                                   
                </ul>
              </li>
            <%end%>
            <% if logged_in?%>
              <li><%= link_to "Users", users_path %></li>
              
              <% end %>
              <% if logged_in?%>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  Account <b class="caret"></b>
                </a>
                <ul class="dropdown-menu">
                  <li><%= link_to "Profile", current_user %></li>
                  <li><%= link_to "Settings",edit_user_path(current_user)  %></li>
                  <li class="divider"></li>
                  <li>
                    <%= link_to "Log out", logout_path, method: "delete" %>
                  </li>
                </ul>
                <li class="dropdown" data-behavior="notifications" data-notifications='<%= render template: "notifications/index", formats: [:json] %>'>
                  <a class="dropdown-toggle nav-link" type="button" data-behavior="notifications-link" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <%= fa_icon "bell" %> <span data-behavior="unread-count"></span>
                  </a>
                  <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenu1" data-behavior="notification-items">
                    <!--
                    <a class="dropdown-item" href="#">Action</a>
                    <a class="dropdown-item" href="#">Another action</a>
                    <a class="dropdown-item" href="#">Something else here</a>
                    -->
                  </div>
              </li>
            <% else %>
              <li><%= link_to "Log in", login_path %></li>
            <% end %>
          </ul>
        </nav>
      </div>
    </header>