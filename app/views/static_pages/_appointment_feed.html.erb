<% @clientAppointments.each do |appointment| %>
        
    <dl >
    	 <% if current_user.trainer? %>
      <dt><%= appointment.client.name%></dt>
      <% else %>
       <dt><%= appointment.trainer.name %></dt>
       <%end%>
       <dd><%= appointment.appointment_at.strftime("%B %d, %Y, %H:%M") %></dd>
      </dl>
                    
                
          
                        
 <% end %>